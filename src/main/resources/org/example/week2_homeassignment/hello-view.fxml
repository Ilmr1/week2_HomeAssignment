<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="240.0" prefWidth="320.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.12" fx:controller="org.example.week2_homeassignment.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Button fx:id="button1" onAction="#setEN" mnemonicParsing="false" prefHeight="26.0" prefWidth="270.0" text="%button1.text" />
        <Button fx:id="button2" onAction="#setIR" mnemonicParsing="false" prefHeight="26.0" prefWidth="270.0" text="%button2.text" />
        <Button fx:id="button3" onAction="#setJP" mnemonicParsing="false" prefHeight="26.0" prefWidth="267.0" text="%button3.text" />
        <Label fx:id="infoLabel" text="Info Label"/>
    </children>
</VBox>
